{
  "presets": ["@babel/preset-env", "@babel/typescript"],
  "env": {
    "development": {
      "sourceMaps": "inline",
      "retainLines": true
    },
    "production": {
      "minified": true
    }
  },
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ]
  ]
}
