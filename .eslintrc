{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["import", "prettier"],
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/typescript",
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/babel"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error"]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
