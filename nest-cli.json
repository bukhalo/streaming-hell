{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/telegram-bot/src",
  "monorepo": true,
  "root": "apps/telegram-bot",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/telegram-bot/tsconfig.app.json"
  },
  "projects": {
    "telegram-bot": {
      "type": "application",
      "root": "apps/telegram-bot",
      "entryFile": "main",
      "sourceRoot": "apps/telegram-bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/telegram-bot/tsconfig.app.json"
      }
    },
    "vk-bot": {
      "type": "application",
      "root": "apps/vk-bot",
      "entryFile": "main",
      "sourceRoot": "apps/vk-bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vk-bot/tsconfig.app.json"
      }
    },
    "backend": {
      "type": "application",
      "root": "apps/backend",
      "entryFile": "main",
      "sourceRoot": "apps/backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "odesly": {
      "type": "library",
      "root": "libs/odesly",
      "entryFile": "index",
      "sourceRoot": "libs/odesly/src",
      "compilerOptions": {
        "tsConfigPath": "libs/odesly/tsconfig.lib.json"
      }
    }
  }
}