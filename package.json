{
  "name": "@streaming-hell/telegram-bot",
  "version": "0.2.3",
  "description": "Telegram bot for share music with anyone no matter the platform",
  "keywords": [
    "telegram",
    "telegram bot",
    "telegraf"
  ],
  "homepage": "https://github.com/streaming-hell/telegram-bot#readme",
  "bugs": {
    "url": "https://github.com/streaming-hell/telegram-bot/issues",
    "email": "aleksandr@bukhalo.com"
  },
  "license": "GPL-3.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=prod node dist/main.js",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "build": "rm -rf ./dist && NODE_ENV=production tsc",
    "dev": "NODE_ENV=dev nodemon --exec babel-node --extensions '.ts' --inspect src/main.ts",
    "deploy": "env-cmd serverless deploy",
    "docker:build": "docker build -t streaming-hell-telegram-bot:$npm_package_version --build-arg NPM_AUTH_TOKEN=9935ad72403ae4eb6a48c3519592164931f506c5 .",
    "docker:run": "docker run -it --rm -e TELEGRAM_BOT_TOKEN=***REMOVED*** -e MIXPANEL_TOKEN=***REMOVED*** -e ODESLY_API_KEY=***REMOVED*** streaming-hell-telegram-bot:0.2.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "lodash": "^4.17.11",
    "telegraf": "^3.33.3",
    "telegraf-mixpanel": "^1.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@robotmafia-team/prettier-config": "^0.1.2",
    "@types/lodash": "^4.14.148",
    "@types/validator": "^10.11.3",
    "prettier": "^1.16.4",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.2"
  },
  "prettier": "@robotmafia-team/prettier-config"
}
